@using SmartCode
@using SmartCode.Generator.Extensions
@model BuildContext
@{
    var project = Model.Project;
    var buildTask = Model.Build;
    var schema = Model.GetCurrentSchema();
    var table = Model.GetCurrentTable();
    var entity = Model.GetEntityContext().FindEntity(schema, table);
}

namespace @($"{project.Module}.{schema.Name}.Domain.Models")
{
    /// <summary>
    /// @entity.Description
    /// </summary>
    public partial class @entity.ClassName
    {
    }
}